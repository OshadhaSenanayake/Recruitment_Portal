<!DOCTYPE html>
<html>

<% include includes/head.ejs %>

<body id="dashboard">
<main ng-app="Form">
    <% include includes/header.ejs %>
    <nav class="secondary-navbar">
        <div class="nav-wrapper">
            <ul class="tabs">
                <li class="tab col s3"><a href="#test1">DASHBOARD</a></li>
                <li class="tab col s3"><a class="active"  href="#test2">INFORMATION</a></li>
                <li class="tab col s3 disabled"><a href="#test3">FAQ</a></li>
                <ul class="right hide-on-med-and-down">
                    <li>
                        <a href="notifications">
                            <i class="fa fa-bell" aria-hidden="true"></i>
                        </a>
                    </li>
                    <li>
                        <a href="/logout">
                            <i class="fa fa-sign-out" aria-hidden="true"></i>
                        </a>
                    </li>
                </ul>

            </ul>

        </div>
    </nav>
    <br><br>


    <div id="test1" class="vacancy container">
        <div class="section">
            <div class="card table-title">VACANCIES</div>
            <table class="card responsive-table bordered centered">
                <thead>
                <tr class="center">
                    <th>Designation</th>
                    <th>Basis</th>
                    <th>Last Date</th>
                    <th>Status</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td>
                        <h6>
                            Assistant Professor
                        </h6>
                    </td>
                    <td class="grey-text">Contractual</td>
                    <td class="grey-text">16 June 2017</td>
                    <td>
                        <a href="#!" class="btn waves-effect waves-teal">APPLY NOW</a>
                    </td>
                </tr>

                <tr>
                    <td>
                        <h6>
                            Assistant Professor
                        </h6>
                    </td>
                    <td class="grey-text">Contractual</td>
                    <td class="grey-text">16 June 2017</td>
                    <td>
                        <a href="#!" class="btn waves-effect waves-teal">APPLY NOW</a>
                    </td>
                </tr>

                <tr>
                    <td>
                        <h6>
                            Lab Assistant
                        </h6>
                    </td>
                    <td class="grey-text">Contractual</td>
                    <td class="grey-text">16 June 2017</td>
                    <td>
                        <a href="#!" class="btn waves-effect waves-teal disabled">APPLY NOW</a>
                    </td>
                </tr>

                </tbody>
            </table>
        </div>
    </div>

    <div  ng-controller="fc" ng-init="selected=0" id="test2" class="info container">
        <div class="card">
            <div class="table-title">INFORMATION</div>
            <div class="row border-top">
                <div class="col m3">
                    <ul class="flexed">
                        <li ng-repeat="info in attributes">
                            <div ng-click="setSelected(info.val)" ng-class="{'grey-text':!isSelected(info)}" class="time">{{ info.key }}</div>
                        </li>
                    </ul>
                </div>
                <div class="col m9">
                    <div ng-show="selected==0" class="container">
                        <br>
                        <div class="row">
                            <div class="input-field col s6">
                                <input ng-model="first_name" placeholder="First Name" id="first_name" type="text">
                            </div>
                            <div class="input-field col s6">
                                <input ng-model="last_name" placeholder="Last Name" id="last_name" type="text">
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col s6">
                                <input ng-model="fhf_name" type="text" placeholder="Father's First Name" name="cand_parent_fname">
                            </div>
                            <div  class="input-field col s6">
                                <input ng-model="fhl_name" type="text" placeholder="Father's Last Name" name="cand_parent_lname">
                            </div>
                        </div>


                        <p class = "center">Discipline (Fill separate Application form for each discipline applied for)</p>
                        <div class="row">
                            <div  class="ss-item-required input-field col s6">
                                <p>
                                    <input ng-model="cse" type="checkbox" class="filled-in" id="filled-in-box1" name="discipline[]" value="cse" />
                                    <label for="filled-in-box1">CSE/IT</label>
                                </p>
                                <p>
                                    <input ng-model="ee" type="checkbox" class="filled-in" id="filled-in-box2" name="discipline[]" value="electrical" />
                                    <label for="filled-in-box2">ELECTRICAL</label>
                                    <span class="discipline[]_error"></span
                                </p>
                            </div>
                            <div  class="ss-item-required input-field col s6">
                                <p>
                                    <input ng-model="ece" type="checkbox" class="filled-in" id="filled-in-box3" name="discipline[]" value="ece" />
                                    <label for="filled-in-box3">ELECTRONICS AND COMMUNICATION ENGINEERING(ECE)</label>
                                </p>
                                <p>
                                    <input ng-model="mae" type="checkbox" class="filled-in" id="filled-in-box4" name="discipline[]" value="mae" />
                                    <label for="filled-in-box4">MECHANICAL AND AUTOMATION ENGINEERING(MAE)</label>
                                </p>
                            </div>
                        </div>

                        <div class="row">
                            <div  class="input-field col s6">
                                <input ng-model="aadhar" id="ame" type="text" placeholder="Aadhar Number" class="aadhar_number" name="aadhar_number">
                            </div>
                            <div  class="ss-item-required input-field col s6">
                                <input ng-model="dob" placeholder="Date Of Birth" id="datepicker" type="text" class="datepicker" name="cand_dob">

                            </div>
                        </div>


                    </div>


                    <div class="container" ng-show="selected==1">
                        <div class="row">
                            <div  class="input-field col s9">
                                <textarea ng-model="correspondence_addr" id="textarea2 " placeholder="Correspondence Address" class="materialize-textarea" name="corresp_address"></textarea>

                            </div>
                            <div  class="input-field col s3">
                                <textarea ng-model="corresp_pi" id="textarea1 " placeholder="Pincode" class="materialize-textarea" name="corresp_pin"></textarea>
                            </div>
                        </div>

                        <div class="row">
                            <div  class="ss-item-required input-field col s9">
                                <textarea placeholder="Permanent Address" ng-model="perm_addr" id="textarea3 " class="materialize-textarea" name="perm_address"></textarea>
                            </div>
                            <div  class="ss-item-required input-field col s3">
                                <textarea id="textarea4 " ng-model="perm_pin" class="materialize-textarea valid_pin" placeholder="Pincode" name="perm_pin"></textarea>
                            </div>
                        </div>




                        <div class="row">
                            <div  class="input-field col s6">
                                <input ng-model="mobile" placeholder="Mobile" id="icon_prefix " type="text" class="valid_num" name="mobile_number">

                            </div>
                            <div  class="input-field col s6">
                                <input ng-model="landline" id="icon_telephone " placeholder="Landline" type="text" class="valid_num_ll" name="landline_number">
                            </div>
                        </div>




                        <div class="row">
                            <div  class="ss-item-required input-field col s12">
                                <input placeholder="Email ID" ng-model="email" id="email" type="email" class="" name="cand_email">

                            </div>
                        </div>


                    </div>

                    <div ng-show="selected==2" class="container">
                        <div class="row">
                            <div  class="input-field col s6">
                                <select ng-model="gender" name="gender">
                                    <option value="" disabled selected>Choose your option</option>
                                    <option value="male">Male</option>
                                    <option value="female">Female</option>
                                </select>

                            </div>
                            <div  class="input-field col s6">
                                <select ng-model="marital_status" name="marital_status">
                                    <option value="" disabled selected>Choose your option</option>
                                    <option value="married">Married</option>
                                    <option value="unmarried">Unmarried</option>
                                </select>

                            </div>
                        </div>




                        <div class="row">
                            <div  class="ss-item-required input-field col s12">
                                <input ng-model="nationality" placeholder="Nationality" id="lme" type="text" class="" name="cand_nation">
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s4">
                                <select ng-model="category" name="category">
                                    <option value="" disabled selected>Choose Category</option>
                                    <option value="general">GENERAL</option>
                                    <option value="obc">OBC</option>
                                    <option value="sc">SC</option>
                                    <option value="st">ST</option>
                                    <option value="pwd">PWD</option>
                                </select>
                            </div>
                            <div  class="ss-item-required input-field col s4">
                                <input ng-model="religion" placeholder="Religion" id="rel" type="text" class="" name="cand_religion">
                            </div>
                            <div  class="ss-item-required input-field col s4">
                                <select ng-model="minority" name="minority">
                                    <option value="" disabled selected>Minority Yes/No</option>
                                    <option value="yes">Yes</option>
                                    <option value="no">No</option>
                                </select>

                            </div>
                        </div>






                    </div>
                    <div ng-show="selected==3" class="container">
                        <br>
                        <table>
                            <thead>
                            <tr>
                                <th>Examination</th>
                                <th>University/Board</th>
                                <th>Year of passing/Award</th>
                                <th>Division</th>
                                <th>Percentage/Grade</th>
                                <th>Subject</th>
                            </tr>
                            </thead>

                            <tbody>
                            <tr>
                                <td>Class X</td>
                                <td><input ng-model="x_board" placeholder="E.g CBSE" id="xp" type="text" class="" name="cand_X_board">
                                <td><input ng-model="cand_x_pass_year" id="xb" type="text" class="" name="cand_X_pass_year"></td>
                                <td><select ng-model="cand_x_division" name="cand_X_division">
                                        <option value="" disabled selected>Select</option>
                                        <option value="1">1</option>
                                        <option value="2">2</option>
                                        <option value="2">3</option>
                                    </select></td>
                                <td><input ng-model="cand_x_grade" id="xs" type="text" class="" name="cand_X_grade"></td>
                                <td><input ng-model="cand_x_subject" placeholder="E.g English,Mathematics,Physics ... " id="xd" type="text" class="" name="cand_X_subject"></td>
                            </tr>
                            <tr>





                                <td>Class XII</td>
                                <td><input ng-model="cand_xii_board" placeholder="E.g CBSE" id="xii" type="text" class="" name="cand_XII_board">
                                <td><input ng-model="cand_xii_pass_year" id="xoop" type="text" class="" name="cand_XII_pass_year"></td>
                                <td><select ng-model="cand_xii_division" name="cand_XII_division">
                                        <option value="" disabled selected>Select</option>
                                        <option value="1">1</option>
                                        <option value="2">2</option>
                                        <option value="2">3</option>
                                    </select></td>
                                <td><input ng-model="cand_xii_grade" id="xpppp" type="text" class="" name="cand_XII_grade"></td>
                                <td><input ng-model="cand_xii_subject" placeholder="E.g English,Mathematics,Physics ... " id="xmmmm" type="text" class="" name="cand_XII_subject"></td>
                            </tr>
                            <tr>




                                <td>M.Tech</td>
                                <td><input ng-model="cand_mtech_board" placeholder="College/Univ" id="mp" type="text" class="" name="cand_mtech_board">
                                <td><input ng-model="cand_mtech_pass_year" id="mb" type="text" class="" name="cand_mtech_pass_year"></td>
                                <td><select name="cand_mtech_division" ng-model="cand_mtech_division">
                                        <option value="" disabled selected>Select</option>
                                        <option value="1">1</option>
                                        <option value="2">2</option>
                                        <option value="2">3</option>
                                    </select></td>
                                <td><input ng-model="cand_mtech_grade" id="mq" type="text" class="" name="cand_mtech_grade"></td>
                                <td><input ng-model="cand_mtech_subject" placeholder="E.g English,Mathematics,Physics ... " id="mr" type="text" class="" name="cand_mtech_subject"></td>
                            </tr>
                            <tr>

                                <td>Ph.D</td>
                                <td><input ng-model="cand_phd_board" placeholder="College/Univ" id="ps" type="text" class="" name="cand_phd_board">
                                <td><input ng-model="cand_phd_pass_year" id="pt" type="text" class="" name="cand_phd_pass_year"></td>
                            </tr>
                            </tbody>
                        </table>



                    </div>

                    <div class="container" ng-show="selected==4">
                        <div class="row">
                            <div  class="ss-item-required input-field col s9">
                                <textarea id="textarea5" class="materialize-textarea" ng-model="cand_employer_address" placeholder="Current Employer Address"></textarea>

                            </div>
                            <div  class="ss-item-required input-field col s3">
                                <textarea id="textarea6" class="materialize-textarea valid_pin" ng-model="cand_employer_pin" placeholder="Pincode"></textarea>

                            </div>
                        </div>
                    </div>

                    <div ng-show="selected==5" class="container">
                        <div class="row">
                            <div  class="ss-item-required input-field col s6">
                                <input  id="datepicker" placeholder="Date of submission of PhD Thesis" type="text" class="datepicker" ng-model="cand_phd_thesis_submission">

                            </div>
                            <div  class="ss-item-required input-field col s6">
                                <input id="datepicker" placeholder="Title of Ph.D thesis" type="text" class="" ng-model="cand_phd_thesis_title">

                            </div>
                        </div>




                        <div class="row">
                            <div  class="ss-item-required input-field col s6">
                                <input placeholder="Month and year of passing of NET/GATE" id="mpg" type="text" class="" ng-model="cand_gate_date">

                            </div>
                            <div  class="ss-item-required input-field col s6">
                                <input id="datepicker" placeholder="Corresponding Roll Number" type="text" class="" ng-model="cand_gate_roll_number">

                            </div>
                        </div>





                        <div class="row">
                            <div  class="ss-item-required input-field col s12">
                                <input id="datepicker" placeholder="Field of specialization" type="text" class="" ng-model="cand_specialization">

                            </div>
                        </div>

                    </div>
                    <div ng-show="selected==6" class="container">
                        <p class = "center extra">Name and Address of two persons to whom references can be made</p>
                        <div class="row">
                            <div  class="ss-item-required input-field col s6">
                                <input  id="datepicker" placeholder="Name" type="text" class="" ng-model="cand_ref1_name">
                            </div>
                            <div  class="ss-item-required input-field col s6">
                                <input  id="datepicker" placeholder="Address" type="text" class="" ng-model="cand_ref1_address">
                            </div>
                        </div>


                        <div class="row">
                            <div  class="ss-item-required input-field col s6">
                                <input id="datepicker" type="text" placeholder="Name" class="" ng-model="cand_ref2_name">

                            </div>
                            <div  class="ss-item-required input-field col s6">
                                <input id="datepicker" type="text" placeholder="Address" class="" ng-model="cand_ref2_address">

                            </div>
                        </div>

                    </div>
                    <div ng-show="selected==7"  class="container">

                        <div class="row">
                            <div class="ss-item-required input-field col s12">
                                <input id="extras" placeholder="Any Other Additonal Details" type="text" class="" ng-model="cand_extras">
                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>


</main>
<br><br>
<% include includes/footer.ejs %>

</body>
</html>
<style>
    .info ul.flexed:before {
        content: "";
        position: absolute;
        left: 0.9em;
        width: 3px;
        height: calc(100% - 80px);
        background: #c0392b;
    }

    .info ul.flexed li{
        margin-left:4em;
        margin-bottom: 2em;
        flex: 1;
    }

    .info ul.flexed .time:before{
        content: "";
        position: absolute;
        left: 0.5em;
        width: 1em;
        height: 1em;
        background: crimson;
        border-radius: 100%;
    }

    .info .row {
        margin-left: 0;
        margin-right: 0;
    }

    .info .card .row.border-top {
        border-top: 0.1em solid #e7eaf1;
    }

    .info .row .col.m3 {
        background: #f4f6f9;
        border-right: 0.1em solid #e7eaf1;
    }

    .info input[type=text] {
        background-color: #f3f3f5;
        text-indent: 1em;
        border-bottom: 0;
    }

    .info input[type=text].valid {
        border-bottom: 1px solid #4CAF50;
        box-shadow: 0 1px 0 0 #4CAF50;
    }

    .info ::-webkit-input-placeholder {
        color: #26a69a;
    }

    .info :-moz-placeholder {
        color: #26a69a;
    }

    .info ::-moz-placeholder {
        color: #26a69a;
    }

    .info :-ms-input-placeholder {
        color: #26a69a;
    }
    .info .row {
        display: flex; /* equal height of the children */
    }
    .info .time {
        cursor: pointer;
    }
    .info ul.flexed {
        display: flex;
        flex-direction: column;
        height: 100%;
    }
    .info .select-wrapper span.caret {
        color: black;
        position: absolute;
        right: 0;
        top: 0;
        bottom: 0;
        height: 10px;
        margin: auto 0;
        font-size: 10px;
        line-height: 10px;
        z-index: 999;
        margin-right: 1em;
    }
    .info .m9 .row {
        margin-bottom: 0 !important;
    }
</style>
<script>
    $(document).ready(function () {
        $('select').material_select();
    })
</script>